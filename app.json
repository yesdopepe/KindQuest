{
  "expo": {
    "name": "KindQuest",
    "slug": "KindQuest",
    "scheme": "KindQuest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/KindQuestSplashScreen.png",
      "backgroundColor": "#FEF8DD"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      ["expo-font",
        {
          "fonts": ["./assets/fonts/SpacGrotesk.ttf"]
        }]
    ]
  }
}
